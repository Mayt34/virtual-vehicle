{
  "setup" : [
    "docker compose --file=../etna/docker-compose.yml --env-file=.env --profile=without-devices up -d"
  ],
  "between_runs" : [ "docker compose --file=../etna/docker-compose.yml --env-file=.env --profile=without-devices restart" ],
  "scenarios" : [
    {
      "name" : "fewNodes",
      "timeout" : 80,
      "arguments" : {
        "--map" : "../maps/1.osm",
        "--route" : "default",
        "--wait-at-stop-s" : 4,
        "--speed-override" : 55,
        "--config" : "../default_config.json"
      }
    },
    {
      "name" : "manyNodes",
      "timeout" : 70,
      "arguments" : {
        "--map" : "../maps/2.osm",
        "--route" : "default",
        "--wait-at-stop-s" : 4,
        "--speed-override" : 150,
        "--config" : "../default_config.json"
      }
    },
    {
      "name" : "slowCar",
      "timeout" : 150,
      "arguments" : {
        "--map" : "../maps/1.osm",
        "--route" : "default",
        "--wait-at-stop-s" : 15,
        "--speed-override" : 35,
        "--config" : "../default_config.json"
      }
    }
  ],
  "cleanup" : [
    "docker compose --profile=without-devices --file=../etna/docker-compose.yml down"
  ]
}
